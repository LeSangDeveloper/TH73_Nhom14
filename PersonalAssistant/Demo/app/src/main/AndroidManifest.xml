<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.demolistviewplan">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_work"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_work"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".IntroductionActivity"></activity>
        <activity android:name=".InstructionActivity" />
        <activity android:name=".DateActivity" />

        <receiver
            android:name=".WorkAlarmReceiver"
            android:enabled="true"
            android:exported="false" />

        <activity android:name=".AddWorkActivity" />
        <activity android:name=".EditWorkActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>